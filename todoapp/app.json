{
  "expo": {
    "name": "todoapp",
    "slug": "todoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/noteapplogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/noteapplogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#DFC8B4",
      "dark": {
        "image": "./assets/images/noteapplogo.png",
        "resizeMode": "cover",
        "backgroundColor": "#DFC8B4"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expoisgood.todoapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.expoisgood.todoapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/noteapplogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashblue.png"
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
